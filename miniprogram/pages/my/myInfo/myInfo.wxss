/* miniprogram/pages/my/myInfo/myInfo.wxss */
.part1 navigator{
  width: 100%;
  height: 50px;
  line-height: 50px;
  font-size: 16px;
  border-bottom: 1px solid #eeeeee;
  display: flex;
  justify-content: space-between;
}
.part1 .item1{
  padding: 10px 0;
}
.part1 navigator view{
  width: 100px;
}

.part1 navigator .txtr{
  padding: 5px 0;
  text-align: right;
}

.part1 navigator .txtr1{
  display: flex;
  justify-content: flex-end;
  padding: 5px 0;
}
.part1 navigator .txtr1 .img image{
  width: 40px;
  height: 40px;
  border-radius: 50%;
}
.part1 navigator .txtr1 .img{
  text-align: right;
  padding-right: 10px;
}

.part1 navigator .txtr view.iconfont{
  font-size: 9px;
  color: #b2b2b2;
}

.part1 navigator .txtr2{
  text-align: right;
  font-size: 12px;
  color: #b2b2b2;
} 
.name{
  padding: 30px 0;
}
.name input{
  border-bottom: 1px #b2b3b4 solid;
}

.name .click{
  margin-top: 10px;
}
.name .click view{
  color: #c5c4c4;
  font-size: 10px;
}

.name .click .cun{
  text-align: center;
  display: flex;
  justify-content: center;
}
.name .click .cun button{
  display: inline-block;
  margin-top: 30px;
  width: 100px;
  height: 40px;
  line-height: 40px;
  border-radius: 20px;
  color: #fff;
  background-color: rgb(55, 248, 97);
}
.part1{
  text-align: center;
}
.part1 .txt{
  margin-top: 14px;
  color: #c5c4c4;
  font-size: 14px;
}
.part1 image{
  width: 150px;
  height: 150px;
  border-radius: 30px;
  margin: 30px;
  margin-top: 14px;
}
.part1 view .cun{
  margin-top: 20px;
}
.part1 button{
  border-radius: 20px;
  color: #fff;
  background-color: rgb(55, 248, 97);
}
