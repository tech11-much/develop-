/* miniprogram/pages/my/usercenter/userInfo/userInfo.wxss */
page{
  background-color: #eeeeee;
}

.part1 navigator{
  width: 100%;
  height: 50px;
  line-height: 50px;
  font-size: 16px;
  border-bottom: 1px solid #eeeeee;
  display: flex;
  justify-content: space-between;
}
.part1 .item1{
  padding: 10px 0;
}
.part1 navigator view{
  width: 100px;
}

.part1 navigator .txtr{
  padding: 5px 0;
  text-align: right;
}

.part1 navigator .txtr1{
  display: flex;
  justify-content: flex-end;
  padding: 5px 0;
}
.part1 navigator .txtr1 .img image{
  width: 40px;
  height: 40px;
  border-radius: 10px;
}
.part1 navigator .txtr1 .img{
  text-align: right;
  padding-right: 10px;
}

.part1 navigator .txtr view.iconfont{
  font-size: 9px;
  color: #b2b2b2;
}

.part1 navigator .txtr2{
  text-align: right;
  font-size: 12px;
  color: #b2b2b2;
} 

.part1 navigator .location{
  height: 50px;
  line-height: 50px;
}